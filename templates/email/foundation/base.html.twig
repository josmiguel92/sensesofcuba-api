<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
{% apply spaceless %}
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width">
    <title>{{ subject }}</title>
    <style>{{ source('email/foundation/foundation-emails.css.twig') }}</style>
    <link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet" type="text/css"></head>

    <body>
    <!-- <style> -->
    <table class="body" data-made-with-foundation="">
      <tr>
        <td class="float-center" align="center" valign="top">
          <center data-parsed="">
            <style type="text/css" align="center" class="float-center">
              body,
              html,
              .body {
                background: #f3f3f3 !important;
              }

              .container.header {
                background: #f3f3f3;
              }

              .body-border {
                border-top: 8px solid #fdc416;
              }
            </style>
            <table align="center" class="container header float-center">
              <tbody>
                <tr>
                  <td>
                    <table class="row">
                      <tbody>
                        <tr>
                          <th class="small-12 large-12 columns first last">
                            <table>
                              <tr>
                                <th>
                                  <h1 class="text-center">
                                    {% block title %}{% endblock title %}
                                  </h1>
                                  <center data-parsed="">
                                    <center data-parsed="">
                                    <table align="center" class="menu text-center float-center">
                                      <tr>
                                        <td>
                                          <table>
                                            <tr>
                                              <th class="menu-item float-center"><a href="https://www.sensesofcuba.com/#Profile">About Us</a></th>
                                              <th class="menu-item float-center"><a href="https://www.sensesofcuba.com/#ourTeam">Our Team</a></th>
                                              <th class="menu-item float-center"><a href="https://www.sensesofcuba.com/#ProductSection">Products</a></th>
                                              <th class="menu-item float-center"><a href="https://www.sensesofcuba.com/#ContactUs">Contact Us</a></th>
                                            </tr>
                                          </table>
                                        </td>
                                      </tr>
                                    </table>
                                  </center>
                                  </center>
                                </th>
                                <th class="expander"></th>
                              </tr>
                            </table>
                          </th>
                        </tr>
                      </tbody>
                    </table>
                  </td>
                </tr>
              </tbody>
            </table>
            <table align="center" class="container body-border float-center">
              <tbody>
                <tr>
                  <td>
                    <table class="row">
                      <tbody>
                        <tr>
                          <th class="small-12 large-12 columns first last">
                            <table>
                              <tr>
                                <th>
                                  <table class="spacer">
                                    <tbody>
                                      <tr>
                                        <td height="32px" style="font-size:32px;line-height:32px;">&#xA0;</td>
                                      </tr>
                                    </tbody>
                                  </table>
                                  <center data-parsed=""> <img src="{{ email.image('email/foundation/logo_small.png') }}" align="center" class="float-center"> </center>
                                  <table class="spacer">
                                    <tbody>
                                      <tr>
                                        <td height="16px" style="font-size:16px;line-height:16px;">&#xA0;</td>
                                      </tr>
                                    </tbody>
                                  </table>

                                  {% block message %}
                                  {% endblock message %}


                                  <center data-parsed="">


                                    <table class="spacer">
                                    <tbody>
                                      <tr>
                                        <td height="16px" style="font-size:16px;line-height:16px;">&#xA0;</td>
                                      </tr><tr>
                                        <td height="16px" style="font-size:16px;line-height:16px;">&#xA0;</td>
                                      </tr>
                                    </tbody>
                                  </table>

                                  <table>
                                    <tr>
                                      <td>
                                        <table>
                                          <tr>
                                            <td>
                                              {% block action %}{% endblock action %}
                                            </td>
                                          </tr>
                                        </table>
                                      </td>
                                      <td class="expander"></td>
                                    </tr>
                                  </table>

                                    <table align="center" class="menu float-center">
                                      <tr>
                                        <td>
                                          <table>
                                            <tr>
                                              <th class="menu-item float-center"><small class="small links"><a href="https://www.sensesofcuba.com/">www.sensesofcuba.com</a></small></th>
                                              <th class="menu-item float-center"><small class="small links"><a href="mailto:info@sensesofcuba.com">info@sensesofcuba.com</a></small></th>
                                              <th class="menu-item float-center"><small class="small links"><a href="tel://+5378664734">+53 7866 4734</a></small></th>
                                            </tr>
                                          </table>
                                        </td>
                                      </tr>
                                    </table>
                                  </center>
                                </th>
                                <th class="expander"></th>
                              </tr>
                            </table>
                          </th>
                        </tr>
                      </tbody>
                    </table>
                    <table class="spacer">
                      <tbody>
                        <tr>
                          <td height="16px" style="font-size:16px;line-height:16px;">&#xA0;</td>
                        </tr>
                      </tbody>
                    </table>
                  </td>
                </tr>
              </tbody>
            </table>
          </center>
        </td>
      </tr>
    </table>
  </body>
{% endapply %}
</html>
