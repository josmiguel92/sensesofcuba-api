{
  "name": "client-intranet",
  "version": "1.0.0",
  "description": "Client Intranet for sensesofcuba.com",
  "author": "lexz <lexz.zaldivar@gmail.com>",
  "license": "ISC",
  "private": true,
  "browserslist": [
    "> 0.1%",
    "not dead"
  ],
  "scripts": {
    "watch": "parcel watch ./src/index.html --out-dir ./dist/ --public-url /dist/",
    "build:dev": "set NODE_ENV=development && parcel build ./src/index.html --public-url /dist/ --no-minify --no-content-hash --out-dir ./dist/",
    "build:prod": "parcel build ./src/index.html --public-url . --detailed-report --no-source-maps --no-cache --out-dir ./dist/",
    "deploy": "rm -r -f ../public/dist/ && cp -r ./dist ../public/dist/",
    "copy": "cp -r ./dist/* ../public/"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.2",
    "autoprefixer": "^9.7.3",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.23.7",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "vue": "^2.6.10",
    "vue-i18n": "^8.15.1",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.1.3",
    "vue-spinner": "^1.0.3",
    "vuex": "^3.1.2"
  }
}
